, "src/views/Companies.vue", "src/views/Brands.vue", "src/views/Stores.vue", "src/store/index.js", "src/store/companies.js", "src/store/brands.js", "src/components/brands/OperationCompanyModal.vue", "src/components/brands/operations/EditOneCompany.vue", "src/components/brands/operations/OperationCompanyModal.vue", "src/components/brands/operations/NavOperation.vue"  ],
